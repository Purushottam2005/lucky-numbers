<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:ejb="http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd" version="3.0">
  <enterprise-beans>
    <session>
      <ejb-name>FancyListBean</ejb-name>
      <business-remote>coreservlets.bean.FancyList</business-remote>
      <ejb-class>coreservlets.bean.FancyListBean</ejb-class>
      <session-type>Stateful</session-type>
    </session>
    <session>
      <ejb-name>NumberListServiceBean</ejb-name>
      <business-remote>coreservlets.bean.NumberListService</business-remote>
      <ejb-class>coreservlets.bean.NumberListServiceBean</ejb-class>
      <session-type>Stateless</session-type>
    </session>
    <session>
      <ejb-name>NumberServiceBean</ejb-name>
      <business-remote>coreservlets.bean.NumberService</business-remote>
      <ejb-class>coreservlets.bean.NumberServiceBean</ejb-class>
      <session-type>Stateless</session-type>
    </session>
    
     <session>
      <ejb-name>DataFetchServiceBean</ejb-name>
      <business-remote>coreservlets.bean.DataFetchService</business-remote>
      <ejb-class>coreservlets.bean.DataFetchServiceBean</ejb-class>
      <session-type>Stateless</session-type>
    </session>
    
    <session>
      <ejb-name>UserManagementServiceBean</ejb-name>
      <business-remote>com.management.user.UserManagementService</business-remote>
      <ejb-class>com.management.user.UserManagementServiceBean</ejb-class>
      <session-type>Stateless</session-type>
    </session>
    
    <session>
      <ejb-name>ReaderServiceBean</ejb-name>
      <business-remote>coreservlets.bean.ReaderService</business-remote>
      <ejb-class>coreservlets.bean.ReaderServiceBean</ejb-class>
      <session-type>Stateless</session-type>
    </session>
    
    
  </enterprise-beans>
</ejb-jar>
